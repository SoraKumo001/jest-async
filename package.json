{
  "name": "jest-async",
  "version": "1.0.4",
  "description": "Simplify Jest parallel testing.  \r This allows you to write fast test code.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "scripts": {
    "build": "tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jest",
    "parallel",
    "test"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "devDependencies": {
    "@swc-node/jest": "^1.6.8",
    "@swc/core": "^1.3.93",
    "@types/jest": "^29.5.6",
    "cpy-cli": "^5.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "jest": "*"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "@swc-node/jest"
      ]
    },
    "moduleNameMapper": {
      "jest-async": "<rootDir>/dist/cjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoraKumo001/jest-async.git"
  },
  "bugs": {
    "url": "https://github.com/SoraKumo001/jest-async/issues"
  },
  "homepage": "https://ttis.croud.jp/"
}
