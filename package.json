{
  "name": "jest-async",
  "version": "1.0.3",
  "description": "Simplify Jest parallel testing.  \r This allows you to write fast test code.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b src",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "jest",
    "parallel",
    "test"
  ],
  "author": "SoraKumo",
  "license": "MIT",
  "devDependencies": {
    "@swc-node/jest": "^1.6.6",
    "@swc/core": "^1.3.76",
    "@types/jest": "^29.5.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "jest": "*"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "@swc-node/jest"
      ]
    },
    "moduleNameMapper": {
      "jest-async": "<rootDir>/dist"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoraKumo001/jest-async.git"
  },
  "bugs": {
    "url": "https://github.com/SoraKumo001/jest-async/issues"
  },
  "homepage": "https://ttis.croud.jp/"
}
